# dependencies
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# testing
/coverage

# next.js
/.next/
/out/

# production
/build

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# env files – never commit secrets. .env.example is the only exception (safe template).
.env*
!.env.example
.env.local

# local store data (contains tokens)
/data/

# local sqlite db
/dev.db

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# Prisma generated client (run: npx prisma generate)
/src/generated/prisma

# Checkout extension build and zip (may contain app URL)
/checkout-extension/dist
/checkout-extension/*.zip

# Shoplazza CLI build output (when run from root)
/dist

# reference/demo only – not part of our app
/Shoplazza-REFERENCE
